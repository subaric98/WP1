use master
go
--create database salonzauljepsavanje
use salonzauljepsavanje

create table djelatnica(
	sifra int,
	ime varchar(50),
	prezime varchar(50)

);
create table korisnik(
	sifra int,
	ime varchar(50),
	prezime varchar(50),
	spol varchar(50),
	duzinanoktiju int,
);

create table termin(
	sifra int,
	datum datetime,
	usluga int,
	djelatnica int,
);

create table usluga(
	sifra int,
	cijena decimal(18,2),
	naziv varchar(50),
	korisnik int

);

